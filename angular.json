    },
    "sale-configurator-mfe-e2e": {
      "root": "apps/sale-configurator-mfe-e2e",
      "sourceRoot": "apps/sale-configurator-mfe-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
