{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "yarn"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "dealership-portal": {
      "projectType": "application",
      "root": "apps/dealership-portal",
      "sourceRoot": "apps/dealership-portal/src",
      "prefix": "code-change",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/apps/dealership-portal",
            "index": "apps/dealership-portal/src/index.html",
            "main": "apps/dealership-portal/src/main.ts",
            "polyfills": "apps/dealership-portal/src/polyfills.ts",
            "tsConfig": "apps/dealership-portal/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/dealership-portal/src/assets",
              {
                "glob": "**/*.svg",
                "input": "node_modules/ionicons/dist/ionicons/svg",
                "output": "./svg"
              }
            ],
            "styles": [
              "apps/dealership-portal/src/styles.scss",
              {
                "input": "apps\\dealership-portal/src/theme/variables.scss"
              }
            ],
            "scripts": [],
            "extraWebpackConfig": "apps/dealership-portal/webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/dealership-portal/src/environments/environment.ts",
                  "with": "apps/dealership-portal/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "extraWebpackConfig": "apps/dealership-portal/webpack.prod.config.js"
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "browserTarget": "dealership-portal:build",
            "extraWebpackConfig": "apps/dealership-portal/webpack.config.js",
            "port": 4200
          },
          "configurations": {
            "production": {
              "browserTarget": "dealership-portal:build:production",
              "extraWebpackConfig": "apps/dealership-portal/webpack.prod.config.js"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dealership-portal:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/dealership-portal/src/**/*.ts",
              "apps/dealership-portal/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "outputs": [
            "coverage/apps/dealership-portal"
          ],
          "options": {
            "jestConfig": "apps/dealership-portal/jest.config.js",
            "passWithNoTests": true,
            "extraWebpackConfig": "apps/dealership-portal/webpack.config.js"
          }
        },
        "cap": {
          "builder": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "--help"
          }
        },
        "add": {
          "builder": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "add",
            "packageInstall": true
          },
          "configurations": {
            "ios": {
              "cmd": "add ios"
            },
            "android": {
              "cmd": "add android"
            }
          }
        },
        "copy": {
          "builder": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "copy",
            "packageInstall": false
          },
          "configurations": {
            "ios": {
              "cmd": "copy ios"
            },
            "android": {
              "cmd": "copy android"
            }
          }
        },
        "open": {
          "builder": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "open",
            "packageInstall": false
          },
          "configurations": {
            "ios": {
              "cmd": "open ios"
            },
            "android": {
              "cmd": "open android"
            }
          }
        },
        "sync": {
          "builder": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "sync",
            "packageInstall": true
          },
          "configurations": {
            "ios": {
              "cmd": "sync ios"
            },
            "android": {
              "cmd": "sync android"
            }
          }
        },
        "update": {
          "builder": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "update",
            "packageInstall": true
          },
          "configurations": {
            "ios": {
              "cmd": "update ios"
            },
            "android": {
              "cmd": "update android"
            }
          }
        }
      }
    },
    "dealership-portal-e2e": {
      "root": "apps/dealership-portal-e2e",
      "sourceRoot": "apps/dealership-portal-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/dealership-portal-e2e/cypress.json",
            "tsConfig": "apps/dealership-portal-e2e/tsconfig.e2e.json",
            "devServerTarget": "dealership-portal:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "dealership-portal:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/dealership-portal-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
